{
  "dashboard": {
    "title": "Batch Processing Performance Dashboard",
    "description": "Real-time monitoring of batch document processing with SLA tracking and bottleneck identification",
    "tags": ["batch", "performance", "sla", "miniminimoon"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Batch Throughput (Documents/Hour)",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "batch_throughput_per_hour",
            "legendFormat": "Current Throughput",
            "refId": "A"
          },
          {
            "expr": "170",
            "legendFormat": "SLA Target (170 docs/hr)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never"
            },
            "color": {"mode": "palette-classic"},
            "unit": "docs/hr",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 170, "color": "yellow"},
                {"value": 200, "color": "green"}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "SLA Target (170 docs/hr)"},
              "properties": [
                {"id": "custom.lineStyle", "value": {"fill": "dash", "dash": [10, 10]}},
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}},
                {"id": "custom.lineWidth", "value": 2}
              ]
            }
          ]
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["last", "mean", "min", "max"]},
          "tooltip": {"mode": "multi", "sort": "none"}
        }
      },
      {
        "id": 2,
        "title": "Documents Processed (Total)",
        "type": "stat",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(batch_documents_processed_total)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "blue"}
              ]
            },
            "unit": "short",
            "decimals": 0
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "textMode": "auto",
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]}
        }
      },
      {
        "id": 3,
        "title": "Current Throughput Rate",
        "type": "stat",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "batch_throughput_per_hour",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 170, "color": "yellow"},
                {"value": 200, "color": "green"}
              ]
            },
            "unit": "docs/hr",
            "decimals": 1
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "background",
          "textMode": "value_and_name",
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]}
        }
      },
      {
        "id": 4,
        "title": "Queue Depth",
        "type": "stat",
        "gridPos": {"x": 12, "y": 4, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "queue_depth",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 1000, "color": "yellow"},
                {"value": 5000, "color": "red"}
              ]
            },
            "unit": "short",
            "decimals": 0
          }
        },
        "options": {
          "graphMode": "area",
          "colorMode": "background",
          "textMode": "value_and_name",
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]}
        }
      },
      {
        "id": 5,
        "title": "Success Rate",
        "type": "gauge",
        "gridPos": {"x": 18, "y": 4, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(batch_documents_processed_total{status=\"success\"}[5m]) / rate(batch_documents_processed_total[5m]) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 90, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "decimals": 2
          }
        },
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        }
      },
      {
        "id": 6,
        "title": "P95 Document Processing Latency",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "batch_document_processing_latency_seconds{quantile=\"0.95\"}",
            "legendFormat": "P95 Latency",
            "refId": "A"
          },
          {
            "expr": "21.2",
            "legendFormat": "SLA Target (21.2s)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never"
            },
            "color": {"mode": "palette-classic"},
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 18.0, "color": "yellow"},
                {"value": 21.2, "color": "red"}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "SLA Target (21.2s)"},
              "properties": [
                {"id": "custom.lineStyle", "value": {"fill": "dash", "dash": [10, 10]}},
                {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}},
                {"id": "custom.lineWidth", "value": 2}
              ]
            }
          ]
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["last", "mean", "max"]},
          "tooltip": {"mode": "multi", "sort": "none"}
        }
      },
      {
        "id": 7,
        "title": "Worker Utilization",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "worker_utilization_percentage",
            "legendFormat": "Worker {{worker_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 1,
              "fillOpacity": 20,
              "showPoints": "never"
            },
            "color": {"mode": "palette-classic"},
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 25, "color": "yellow"},
                {"value": 50, "color": "green"},
                {"value": 95, "color": "yellow"}
              ]
            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "right", "calcs": ["last", "mean"]},
          "tooltip": {"mode": "multi", "sort": "desc"}
        }
      },
      {
        "id": 8,
        "title": "Processing Stage Bottleneck Heatmap",
        "type": "heatmap",
        "gridPos": {"x": 0, "y": 16, "w": 24, "h": 10},
        "targets": [
          {
            "expr": "sum(rate(batch_stage_processing_time_seconds_bucket[5m])) by (le, stage)",
            "format": "heatmap",
            "legendFormat": "{{stage}}",
            "refId": "A"
          }
        ],
        "options": {
          "calculate": false,
          "calculation": {},
          "cellGap": 2,
          "cellRadius": 0,
          "color": {
            "exponent": 0.5,
            "fill": "dark-orange",
            "mode": "scheme",
            "reverse": false,
            "scale": "exponential",
            "scheme": "Spectral",
            "steps": 128
          },
          "exemplars": {"color": "rgba(255,0,255,0.7)"},
          "filterValues": {"le": 1e-9},
          "legend": {"show": true},
          "rowsFrame": {"layout": "auto"},
          "tooltip": {"show": true, "yHistogram": false},
          "yAxis": {
            "axisPlacement": "left",
            "reverse": false,
            "unit": "s"
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "hideFrom": {"tooltip": false, "viz": false, "legend": false},
              "scaleDistribution": {"type": "linear"}
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Error Rate by Category",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 26, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(batch_documents_processed_total{status=\"error\"}[5m]) * 100",
            "legendFormat": "{{error_category}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 0,
              "showPoints": "auto",
              "stacking": {"mode": "normal", "group": "A"}
            },
            "color": {"mode": "palette-classic"},
            "unit": "percent"
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["last", "sum"]},
          "tooltip": {"mode": "multi", "sort": "desc"}
        }
      },
      {
        "id": 10,
        "title": "Documents in Processing Stages",
        "type": "bargauge",
        "gridPos": {"x": 12, "y": 26, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "batch_stage_documents_active",
            "legendFormat": "{{stage}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 50, "color": "yellow"},
                {"value": 100, "color": "red"}
              ]
            },
            "unit": "short",
            "min": 0
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true,
          "text": {}
        }
      },
      {
        "id": 11,
        "title": "Queue Growth Rate",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 34, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "rate(queue_depth[5m])",
            "legendFormat": "{{queue_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never"
            },
            "color": {"mode": "palette-classic"},
            "unit": "docs/sec"
          }
        },
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"},
          "tooltip": {"mode": "multi", "sort": "none"}
        }
      },
      {
        "id": 12,
        "title": "SLA Compliance Status",
        "type": "stat",
        "gridPos": {"x": 12, "y": 34, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "(batch_throughput_per_hour >= 170) and (batch_document_processing_latency_seconds{quantile=\"0.95\"} <= 21.2)",
            "legendFormat": "SLA Compliant",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            },
            "mappings": [
              {"type": "value", "value": "0", "text": "❌ SLA VIOLATION"},
              {"type": "value", "value": "1", "text": "✅ SLA COMPLIANT"}
            ]
          }
        },
        "options": {
          "graphMode": "none",
          "colorMode": "background",
          "textMode": "value",
          "reduceOptions": {"values": false, "calcs": ["lastNotNull"]}
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "query": "prometheus",
          "current": {
            "text": "Prometheus",
            "value": "Prometheus"
          }
        },
        {
          "name": "worker",
          "type": "query",
          "datasource": "$datasource",
          "query": "label_values(worker_utilization_percentage, worker_id)",
          "multi": true,
          "includeAll": true,
          "refresh": 2
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "SLA Violations",
          "datasource": "$datasource",
          "enable": true,
          "expr": "ALERTS{alertname=~\".*SLA.*\", alertstate=\"firing\"}",
          "iconColor": "red",
          "tags": ["sla", "alert"]
        }
      ]
    },
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}
