[run]
source = .
omit =
    venv/*
    */tests/*
    */test_*
    *_test.py
    setup.py
    */site-packages/*
    .git/*
    */demo*.py
    */example*.py
    exportToHTML/*
    out/*
    output/*
    reports/*
branch = True
parallel = True

[report]
precision = 2
show_missing = True
skip_empty = True
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

[html]
directory = htmlcov

[json]
output = coverage.json
pretty_print = True
